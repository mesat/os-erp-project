<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">

    <class>com.proline.OsErpProline.entity.Contact</class>
    <class>com.proline.OsErpProline.entity.Employee</class>
    <class>com.proline.OsErpProline.entity.Leader</class>
    <class>com.proline.OsErpProline.entity.Socialmedia</class>
    <class>com.proline.OsErpProline.entity.Team</class>
    <class>com.proline.OsErpProline.entity.TeamMember</class>
    <properties>
      <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/employee"/>
      <property name="hibernate.connection.driver_class" value="com.mysql.cj.jdbc.Driver"/>
    </properties>
  </persistence-unit>
    <persistence-unit name="persistenceUnit"/>
</persistence>
